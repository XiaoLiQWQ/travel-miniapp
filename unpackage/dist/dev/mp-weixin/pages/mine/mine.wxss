
.user-info {
		box-sizing: border-box;
		padding: 20px;
		width: 100%;
		display: flex;
		align-items: center;
}
.user-info img {
		margin: 15px;
		width: 50px;
		height: 50px;
}
.user-info-content {
		display: flex;
		flex-direction: column;
}
.nick {
		font-weight: bold;
}
.level {
		font-size: 14px;
		font-weight: bold;
		color: #666;
}
.login {
		width: 80%;
}

